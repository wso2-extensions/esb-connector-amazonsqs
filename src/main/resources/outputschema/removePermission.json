{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "RemovePermission Response Schema",
  "description": "Output schema for removePermission operation",
  "properties": {
    "payload": {
      "type": "object",
      "properties": {
        "ResponseMetadata": {
          "type": "object",
          "properties": {
            "RequestId": {
              "type": "string",
              "description": "The unique request ID returned by AWS"
            }
          },
          "required": ["RequestId"]
        },
        "RemovePermissionResult": {
          "type": "object",
          "description": "Empty result object for successful permission removal"
        },
        "success": {
          "type": "boolean",
          "description": "Indicates if the operation was successful"
        }
      },
      "required": ["ResponseMetadata", "RemovePermissionResult", "success"]
    }
  },
  "required": ["payload"]
}
