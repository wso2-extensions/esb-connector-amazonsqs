{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "PurgeQueue Response Schema",
  "description": "Output schema for purgeQueue operation",
  "properties": {
    "payload": {
      "type": "object",
      "properties": {
        "ResponseMetadata": {
          "type": "object",
          "properties": {
            "RequestId": {
              "type": "string",
              "description": "The unique request ID returned by AWS"
            }
          },
          "required": ["RequestId"]
        },
        "PurgeQueueResult": {
          "type": "object",
          "description": "Empty result object for successful purge"
        }
      },
      "required": ["ResponseMetadata", "PurgeQueueResult"]
    }
  },
  "required": ["payload"]
}
