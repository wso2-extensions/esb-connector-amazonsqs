                {
                  "type": "attribute",
                  "value": {
                    "name": "apiCallAttemptTimeout",
                    "displayName": "API Call Attempt Timeout",
                    "inputType": "integerOrExpression",
                    "defaultValue": "",
                    "required": "false",
